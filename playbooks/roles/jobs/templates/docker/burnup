#!/bin/bash

echo "burnup jobs:"

mkdir -p {{ tapisdatadir }}/jobs

# myuid=`id -u`
export UID=`id -u`
export GID=`id -g`
docker run -it --rm -v {{ tapisdatadir }}/jobs:/jobs tapis/ubutil2204:1.4.0 chown $UID /jobs

python3 {{ tapisdir }}/admin/util/parse_skexport -c jobs -d {{ tapisdatadir }}

docker compose up -d

